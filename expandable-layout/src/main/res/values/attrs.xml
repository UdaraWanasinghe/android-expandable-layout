<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="expanded" format="boolean" />
        <attr name="duration" format="integer" />
        <attr name="expandDirection" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="interpolator" format="enum">
            <enum name="accelerate_decelerate" value="0" />
            <enum name="accelerate" value="1" />
            <enum name="anticipate" value="2" />
            <enum name="anticipate_overshoot" value="3" />
            <enum name="bounce" value="4" />
            <enum name="cycle" value="5" />
            <enum name="decelerate" value="6" />
            <enum name="overshoot" value="7" />
            <enum name="linear" value="8" />
        </attr>
    </declare-styleable>
</resources>